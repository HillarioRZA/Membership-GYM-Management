
        
        <div class="row">
          <div class="col-md-12">
            <div class="d-flex align-items-center mb-4">
              <h3 class="mb-1 font-weight-bold">Register Form</h3>
            </div>
          </div>
        </div>
        <% if(typeof errors != 'undefined'){ %>
          <div class="alert alert-danger d-flex align-items-center" role="alert">
            <ul>
              
                <li><%= errors[0].msg %></li>
              
            </ul>
          </div>
          <% } %>
        <hr class="horizontal mb-3 dark" />
        <div class="row">
          <div class="col-md-0 mb-7">
            <form method="post">
              <div class="form-group">
                <label class="form-control-label">Nama</label>
                <input
                  class="form-control"
                  type="text"
                  name="nama_reg"
                  placeholder="Budi "
                />
              </div>
              <div class="form-group">
                <label class="form-control-label">Alamat</label>
                <input
                  class="form-control"
                  type="text"
                  name="alamat_reg"
                  placeholder="Puri Indah Blok ** No **"
                />
              </div>
              <div class="form-group">
                <label class="form-control-label">No Telpon</label>
                <input
                  class="form-control"
                  type="number"
                  name="no_telp_reg"
                  placeholder="081234567890"
                />
              </div>
              <div class="form-group">
                <label class="form-control-label">Periode Member</label>
                <br />
                <input
                  class="form-control"
                  type="number"
                  placeholder="5 Bulan"
                  name="Period_member_reg"
                />
              </div>
              <button
              type="submit"
              style="margin-left: 90%"
              class="btn btn-primary"
            >
              Save Member
            </button>
            </form>
          </div>
        </div>